{
  "AllowedHosts": "*",
  "AllowedOrigins": ["http://localhost:3000"],
  "ConnectionStrings": {
    "RedisCache": ""
  },
  "DnsService": {
    "DefaultZone": "",
    "Url": ""
  },
  "Identity": {
    "ApplicationName": "unifi.ipmanager",
    "Authority": "https://auth.mattgerega.net",
    "Enabled": true
  },
  "IpOptions": {
    "IpCooldownMinutes": "240",
    "IpGroups": [
      {
        "Blocks": [
          {
            "Max": "35",
            "Min": "1"
          }
        ],
        "Name": "physical"
      },
      {
        "Blocks": [
          {
            "Max": "79",
            "Min": "36"
          }
        ],
        "Name": "virtual"
      },
      {
        "Blocks": [
          {
            "Max": "89",
            "Min": "80"
          }
        ],
        "Name": "camera"
      },
      {
        "Blocks": [
          {
            "Max": "120",
            "Min": "90"
          }
        ],
        "Name": "enduser"
      }
    ]
  },
  "Logging": {
    "OpenTelemetry": {
      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning"
    }
  },
  "Telemetry": {
    "ActivitySourceName": "Unifi.IpManager.Activity",
    "Log": {
      "Otlp": {
        "Endpoint": "",
        "Protocol": "http"
      },
      "Type": "otlp"
    },
    "MeterName": "Unifi.IpManager.Meter",
    "Metrics": {
      "Otlp": {
        "Endpoint": "",
        "Protocol": "http"
      },
      "Type": "otlp"
    },
    "ServiceName": "Unifi.IpManager",
    "Trace": {
      "Otlp": {
        "Endpoint": ""
      },
      "Type": "otlp"
    }
  },
  "UnifiControllerOptions": {
    "Url": "https://unifi.gerega.net/"
  }
}
