{
  "AllowedHosts": "*",
  "AllowedOrigins": ["https://localhost:7800"],
  "ConnectionStrings": {
    "RedisCache": ""
  },
  "Identity": {
    "ApplicationName": "unifi.ipmanager",
    "Authority": "https://auth.mattgerega.com",
    "Enabled": true
  },
  "IpOptions": {
    "IpCooldownMinutes": "240"
  },
  "Logging": {
    "OpenTelemetry": {
      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning"
    }
  },
  "Telemetry": {
    "ActivitySourceName": "Unifi.IpManager.Activity",
    "Log": {
      "Otlp": {
        "Endpoint": "",
        "Protocol": "http"
      },
      "Type": "otlp"
    },
    "MeterName": "Unifi.IpManager.Meter",
    "Metrics": {
      "Otlp": {
        "Endpoint": "",
        "Protocol": "http"
      },
      "Type": "otlp"
    },
    "ServiceName": "Unifi.IpManager",
    "Trace": {
      "Otlp": {
        "Endpoint": ""
      },
      "Type": "otlp"
    }
  },
  "UnifiControllerOptions": {
    "Url": "https://unifi.gerega.net/"
  }
}
